<%@ page language="java" contentType="text/html; charset=ISO-8859-1"	pageEncoding="ISO-8859-1"%><!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><%@ include file="/pages/header/header.jsp"%><html><head><title>Untitled Document</title><meta content="text/html; charset=us-ascii" http-equiv="Content-Type"><script type="text/javascript" src="datepair.js"></script><script type="text/javascript" src="/DoctorsOnline/resources/js/jquery.timepicker.js"></script><script>    // initialize input widgets first    $('#datepairExample .time').timepicker({        'showDuration': true,        'timeFormat': 'g:ia'    });    $('#datepairExample .date').datepicker({        'format': 'yyyy-m-d',        'autoclose': true    });    // initialize datepair    $('#datepairExample').datepair();</script></head><body>	<!-- DC Form Beautify Start -->	<form id="appointmentform" method="post" name="appointmentform"		action="#">		<table			style="BORDER-BOTTOM: #ccc 1px dotted; BORDER-LEFT: #ccc 1px dotted; PADDING-BOTTOM: 25px; PADDING-LEFT: 25px; PADDING-RIGHT: 25px; BORDER-TOP: #ccc 1px dotted; BORDER-RIGHT: #ccc 1px dotted; PADDING-TOP: 25px"			border="0" cellspacing="0" cellpadding="6" bgcolor="#FFFFFF">			<tbody>				<tr>					<td><label>Select Facility :</label></td>					<td><select name="Facilityselect">							<option selected value="salesdept">Bansodha Chulna</option>							<option value="supportdept">Vikas Clinic</option>					</select></td>				</tr>				<tr>					<td><label>Select Doctor :</label></td>					<td><select name="DoctorSelect">							<option selected value="1">Dr Bansodha</option>							<option value="2">Dr Aputikar</option>					</select></td>				</tr>				<tr>					<td><label>Patient name or Online Id</label></td>					<td><input size="25" type="patientidword"></td>				</tr>				<tr>					<td><label>Select Reason</label></td>					<td><select name="reason">							<option selected value="1">Fever</option>							<option value="2">Cold</option>					</select></td>				</tr>				<tr>				<td>Select Appointment Time </td>				<td>					<p id="datepairExample">						<input type="text" class="date start" /> <input type="text"							class="time start" /> to <input type="text" class="time end" />						<input type="text" class="date end" />					</p>					</td>				</tr>			</tbody>		</table>	</form></body></html>